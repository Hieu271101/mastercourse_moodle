<?xml version="1.0" encoding="UTF-8"?>
<XMLDB PATH="local/mastercourse/db" VERSION="2020071904" COMMENT="XMLDB file for master course plugin"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
    <TABLES>
        <TABLE NAME="user_enrol_mastercourse" COMMENT="table for user enrolment in master courses">
            <FIELDS>
                <FIELD NAME="id" TYPE="bigint" LENGTH="20" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="id_mastercourse" TYPE="bigint" LENGTH="20" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
                <FIELD NAME="id_user" TYPE="bigint" LENGTH="20" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>

        <TABLE NAME="course_master" COMMENT="table for master courses">
            <FIELDS>
                <FIELD NAME="id" TYPE="bigint" LENGTH="20" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="name" TYPE="string" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="role_id" TYPE="bigint" LENGTH="20" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
                <FIELD NAME="status" TYPE="bigint" LENGTH="20" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
                <FIELD NAME="sortorder" TYPE="bigint" LENGTH="20" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
                <FIELD NAME="enrolstartdate" TYPE="bigint" LENGTH="20" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
                <FIELD NAME="enrolenddate" TYPE="bigint" LENGTH="20" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>
